<PropertyList>
 
<path>gtav-declasse-voodoo.ac</path>
  <nasal>
    <load>
      var livery_update = aircraft.livery_update.new("Aircraft/gtav-declasse-voodoo/Models/Liveries", 10);
    </load>

    <unload>
      livery_update.stop();
    </unload>
  </nasal>
<effect>
        <inherits-from>Aircraft/Generic/Effects/Fuselagereflect</inherits-from>
        <object-name>Chrome</object-name>
</effect>

<animation>
    <type>material</type>
    <object-name>Body</object-name>
    <property-base>sim/model/livery</property-base>
    <texture-prop>texture</texture-prop>
    <texture>Kerdi Cadmium Green.png</texture>
  </animation>


<!-- FRONT WHEEL LEFT -->

<animation>
 <type>rotate</type>
 <object-name>Tyre1</object-name>
 <property>gear/gear[0]/steering-norm</property>
 <factor>-45</factor>
 <center>
  <x-m> -1.73276</x-m>
  <y-m>-0.90576</y-m>
  <z-m> 0.3303</z-m>
 </center>
 <axis>
  <x>0.0</x>
  <y>0.0</y>
  <z>1.0</z>
 </axis>
</animation>

<animation>
 <type>translate</type>
 <object-name>Tyre1</object-name>
 <property>gear/gear[0]/compression-norm</property>
 <interpolation>
  <entry>
   <ind>0.0</ind>	
   <dep>0</dep>
  </entry>
  <entry>	
	<ind>1.00</ind>
	<dep>0.2</dep>
  </entry>  
 </interpolation>
 <axis>
  <x>0</x>	
  <y>0</y>
  <z>1</z>	
 </axis>
</animation>
 
<animation>
 <type>spin</type>
 <object-name>Tyre1</object-name>
 <property>gear/gear[2]/rollspeed-ms</property>
 <factor>-15</factor>
 <center>
  <x-m>-1.73276</x-m>
  <y-m>-0.90576</y-m>
  <z-m>0.3303</z-m>
 </center>
 <axis>
  <x>0.0</x>
  <y>1.0</y>
  <z>0.0</z>
 </axis>
</animation>

